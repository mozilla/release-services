{
  "name": "relengapi_tools",
  "version": "0.1.0",
  "description": "relengapi frontend",
  "main": "src/index.js",
  "author": "Rok Garbas <rok@garbas.si>",
  "repository": "https://github.com/mozilla/build-relengapi",
  "license": "MPL-2.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "config": {
    "entry": "src/run.js",
    "ghooks": {
      "pre-commit": "npm test"
    },
    "html": {
      "title": "RelengAPI Tools",
      "description": "Collection of Mozilla Release Engineering Tools",
      "author": "Mozilla RelEng Team",
      "head": "<link href=\"https://fonts.googleapis.com/css?family=Fira+Sans:400,300,500\" rel=\"stylesheet\" type=\"text/css\"\\>"
    }
  },
  "scripts": {
    "build": "neo build",
    "start": "neo start --port 5500",
    "test": "neo test"
  },
  "keywords": [
    "mozilla",
    "relengapi"
  ],
  "dependencies": {
    "bootstrap": "4.0.0-alpha.2",
    "classnames": "^2.2.5",
    "expose-loader": "0.7.1",
    "ghooks": "1.2.1",
    "jquery": "2.2.3",
    "mozilla-neo": "git://github.com/garbas/neo.git",
    "react": "15.0.2",
    "react-addons-test-utils": "15.0.2",
    "react-dom": "15.0.2",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
    "react-router-redux": "4.0.4",
    "redux": "3.5.2",
    "redux-saga": "^0.10.4",
    "tether": "1.3.2"
  },
  "devDependencies": {
    "chai-immutable": "^1.5.4"
  }
